
<div class="jumbotron jcolor">
  <div class="container">
    <h1><span class="fa fa-industry"></span> Add New Benchmark</h1>
    <p>Enter a Benchmark Favorite Chart ID and Name</p>
  </div>
</div>

<div class="container">
  <form role="form" action="/index/create" method="POST">
    <div class="row">
      <div class="form-group col-md-3">
        <label >Favorite Chart ID</label>
        <input type="text" class="form-control" id="cid" name="cid" placeholder="ID">
        <label >Favorite Chart Name</label>
        <input type="text" class="form-control" id="name" name="name" placeholder="Name">
      </div>
    </div>
    <div class="row">
      <div class="col-md-3">
        <button type="submit" class="btn btn-primary">Submit</button>
      </div>
    </div>
  </form>
</div>
